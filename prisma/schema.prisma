generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url = env("DATABASE_URL")
}

model Tests {
    id Int @id @unique @default(autoincrement())
    name String
    description String
    status String
    category String
    result Int
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}